{
  "name": "@xwfintech/logger",
  "version": "0.0.0",
  "description": "",
  "main": "./lib/koa.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register test/index.ts",
    "dev:koa": "ts-node -r tsconfig-paths/register test/koa.ts",
    "build": "tsc -p tsconfig.json",
    "test": "",
    "commit": "git add -A && git-cz",
    "release-major": "yarn build && standard-version -r major",
    "release-minor": "yarn build && standard-version -r minor",
    "release-patch": "yarn build && standard-version -r patch"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.xwfintech.com/youngluo/logger.git"
  },
  "keywords": [
    "logger"
  ],
  "author": "young",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/koa": "^2.0.48",
    "@types/lodash": "^4.14.124",
    "@types/moment": "^2.13.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^2.2.0",
    "koa": "^2.7.0",
    "lint-staged": "^8.1.6",
    "shx": "^0.3.2",
    "standard-version": "^6.0.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "uuid": "^3.3.2"
  }
}
